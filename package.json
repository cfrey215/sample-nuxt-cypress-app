{
  "name": "sample-nuxt-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "export": "nuxt export",
    "serve": "nuxt serve",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test": "jest",
    "cy:test": "cypress run",
    "test:report": "cypress run && npm run cov:report && npm run open:report",
    "cov:report": "npx nyc report --reporter text --reporter lcov",
    "open:report": "open 'cypress-coverage/lcov-report/index.html'"
  },
  "dependencies": {
    "nuxt": "^2.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@cypress/code-coverage": "^3.8.1",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^4.9.0",
    "eslint": "^7.2.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "jest": "^26.0.1",
    "nyc": "^15.1.0",
    "vue-jest": "^3.0.4"
  }
}
